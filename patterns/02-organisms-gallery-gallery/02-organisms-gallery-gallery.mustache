
{{#gallery}}
&lt;div class=&quot;gallery&quot; {{^is_local_dev?}}data-js-component=&quot;gallery&quot;{{/is_local_dev?}}
     data-config=&quot;galleryConfig&quot;
     data-bind=&quot;swipeLeft: gotoNextSlide,
                swipeRight: gotoPrevSlide,
                leftArrowPress: gotoPrevSlide,
                rightArrowPress: gotoNextSlide&quot;&gt;

    &lt;div class=&quot;gallery__slide&quot;&gt;
      {{&gt; organisms-gallery-controls }}

      &lt;div class=&quot;gallery__media&quot;
           data-bind=&quot;template: {
            if: currentSlide,
            name: templateType,
            data: currentSlide()}, css: slideMediaType&quot;&gt;

            {{#slide}}

              {{#image}}
                {{&gt; organisms-gallery-image }}
              {{/image}}

              {{#video}}
                {{&gt; organisms-gallery-video }}
              {{/video}}

              {{#ad}}
                {{&gt; organisms-gallery-ad }}
              {{/ad}}

            {{/slide}}
      &lt;/div&gt;

      &lt;!-- ko if: isNotAdSlide --&gt;
        {{&gt; molecules-social-share }}
      &lt;!-- /ko --&gt;


    &lt;/div&gt;

    &lt;!-- ko if: isNotAdSlide --&gt;
    {{&gt; organisms-gallery-meta }}
    &lt;!-- /ko --&gt;
&lt;/div&gt;
&lt;script type=&quot;text/html&quot; id=&quot;image-slide-template&quot;&gt;
  &lt;img class=&quot;gallery__image&quot; src=&quot;&quot; alt=&quot;&quot; data-pin-no-hover=&quot;true&quot;  data-bind=&quot;attr: {src: image, alt: title}&quot;&gt;
&lt;/script&gt;

&lt;script type=&quot;text/html&quot; id=&quot;video-slide-template&quot;&gt;
    &lt;div class=&quot;gallery__video&quot;&gt;
      &lt;div style=&quot;display:none&quot;&gt;&lt;/div&gt;
      &lt;object type=&quot;application/x-shockwave-flash&quot; data=&quot;http://c.brightcove.com/services/viewer/federated_f9?&amp;amp;width=640&amp;amp;height=360&amp;amp;flashID=myExperience-1&amp;amp;wmode=opaque&amp;amp;bgcolor=%23FFFFFF&amp;amp;playerID=2444926739001&amp;amp;playerKey=AQ~~%2CAAAAABxHNRc~%2C2PQy55JIF_GMqcUDrm_IeQ8yTdYMQ0Fj&amp;amp;includeAPI=true&amp;amp;templateLoadHandler=bcTemplateLoaded&amp;amp;templateReadyHandler=brightcove%5B%22templateReadyHandlermyExperience-1%22%5D&amp;amp;isVid=true&amp;amp;%40videoPlayer=4493023517001&amp;amp;isUI=true&amp;amp;dynamicStreaming=true&amp;amp;linkBaseURL=h&amp;amp;autoStart=&amp;amp;debuggerID=&amp;amp;originalTemplateReadyHandler=onTemplateReady&amp;amp;startTime=1445291546823&quot; id=&quot;myExperience-1&quot; width=&quot;640&quot; height=&quot;360&quot; class=&quot;BrightcoveExperience brightcove-field-embed&quot; seamlesstabbing=&quot;undefined&quot;&gt;&lt;param name=&quot;allowScriptAccess&quot; value=&quot;always&quot;&gt;&lt;param name=&quot;allowFullScreen&quot; value=&quot;true&quot;&gt;&lt;param name=&quot;seamlessTabbing&quot; value=&quot;false&quot;&gt;&lt;param name=&quot;swliveconnect&quot; value=&quot;true&quot;&gt;&lt;param name=&quot;wmode&quot; value=&quot;opaque&quot;&gt;&lt;param name=&quot;quality&quot; value=&quot;high&quot;&gt;&lt;param name=&quot;bgcolor&quot; value=&quot;#FFFFFF&quot;&gt;&lt;/object&gt;
    &lt;/div&gt;
&lt;/script&gt;

&lt;script type=&quot;text/html&quot; id=&quot;ad-slide-template&quot;&gt;
  &lt;div class=&quot;gallery__ad&quot;&gt;
    &lt;div id=&quot;gallery__ad-container&quot; data-bind=&quot;adContainer&quot;&gt;
      &lt;div  class=&quot;ad-placeholder ad-refresh&quot; id=&quot;ad-gallery_interstitial_ad&quot; data-id=&quot;gallery_interstitial_ad&quot; data-type=&quot;regular&quot;&gt;&lt;/div&gt;
    &lt;/div&gt;
  &lt;/div&gt;
&lt;/script&gt;

&lt;script&gt;
  var galleryConfig = {{{config}}};
&lt;/script&gt;
{{/gallery}}